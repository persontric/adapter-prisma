{
	"name": "@persontric/adapter-prisma",
	"version": "4.0.0",
	"description": "Prisma adapter for Lucia",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"module": "dist/index.js",
	"type": "module",
	"files": [
		"*.md",
		"*.js",
		"*.ts",
		"*.json",
		"prisma"
	],
	"scripts": {
		"build": "shx rm -rf ./dist/* && tsc",
		"test": "tsx test.prisma/prisma.ts",
		"test-setup": "prisma db push && prisma generate",
		"auri.build": "pnpm build"
	},
	"keywords": [
		"persontric",
		"prisma",
		"auth",
		"postgres",
		"mysql",
		"sqlite",
		"authentication",
		"adapter",
		"sql",
		"postgresql"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/persontric/adapter-prisma"
	},
	"author": "Brian Takita",
	"license": "Apache-2.0",
	"exports": {
		".": "./index.ts",
		"./package.json": "./package.json"
	},
	"devDependencies": {
		"persontric": "workspace:*",
		"@persontric/adapter-test": "workspace:*",
		"@prisma/client": "^5.14.0",
		"prisma": "^5.14.0",
		"tsx": "^4.10.2"
	},
	"peerDependencies": {
		"@prisma/client": "^5.14.0",
		"persontric": "^0.0.0"
	}
}